# A sample Gemfile
source "https://rubygems.org"
ruby '2.2.3'

 gem 'sinatra'
 gem 'rubygems'
 gem 'sinatra-contrib'
 gem "sinatra-activerecord", :require => 'sinatra/activerecord'
 gem "activerecord"

group :development do
  gem 'sqlite3'
end

group :production do
  gem 'pg'
  gem "activerecord-postgresql-adapter"
end
